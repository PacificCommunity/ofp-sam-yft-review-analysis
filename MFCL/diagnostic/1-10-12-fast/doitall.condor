#  ---------
if [ ! -f 05.par ]; then
 $MFCL yft.frq 04.par 05.par -file - <<PHASE5
 -100000 1 1     # estimate
 -100000 2 1     # time-invariant
 -100000 3 1     # distribution
 -100000 4 1     # of
 -100000 5 1     # recruitment
 -100000 6 1
 -100000 7 1
 -100000 8 1
 -100000 9 1
PHASE5
fi

#  ---------
#   PHASE 6
#  ---------
