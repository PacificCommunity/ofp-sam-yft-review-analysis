 -33  45 100000      # increase penalty on catch for index fisheries
 -34  45 100000 	   
 -35  45 100000 
 -36  45 100000
 -37  45 100000
 -38  45 100000
 -39  45 100000
 -40  45 100000
 -41  45 100000
PHASE4
fi

#  ---------
